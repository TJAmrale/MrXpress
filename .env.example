APP_NAME=Laravel
APP_ENV=local
# Should be generated using 'php artisan key:generate' after copying .env.example to .env
APP_KEY=
APP_DEBUG=true #TODO: Change to FALSE in production environment
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=mrxpress
DB_USERNAME=root
#TODO: Set your database password
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
